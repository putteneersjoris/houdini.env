opparm $_obj_machine_vision1 python ( 'import os\nimport glob\nimport subprocess\nimport cv2\nimport hou\n\n\nnode = hou.pwd()\n\n\ncam = node.input(0)\n\ntemp_image_path = "\\$TEMP/mv_\\$F3.png"\nendFrame = $FEND\nstartFrame = $FSTART\n\ndef destroy_children(node):\n    # destroy all children\n    for n in node.children(): n.destroy(True)\n\n\ndef create_ropnet_and_opengl(node, cam, temp_image_path):\n    rop = node.createNode("ropnet", "ropnet")\n    rop.layoutChildren()\n    opengl = rop.createNode("opengl", "opengl")\n    opengl.setParms({\'camera\': cam.path(), \'trange\':1, \'f1\':"$FSTART",\'f2\':"$FEND"})\n    opengl.parm("picture").set(temp_image_path)\n    return opengl\n\n\ndef render_opengl(node): \n    # opengl = hou.node(node.path()+"/geometry/opengl")\n    opengl = hou.node(node.path()+"/ropnet/opengl")\n    opengl.parm("execute").pressButton()\n\ndef render_video():\n    # render video\n    # print("rendering video")\n    os.system("ffmpeg -r 24 -i $TEMP/mv_%03d.png -vcodec libx264 -crf 25 -pix_fmt yuv420p $TEMP/mv.mp4")\n\n\ndef remove_temp_files():\n    # remove all images\n    temp_path_img = \'$TEMP/*.png\'\n    temp_files = glob.glob(temp_path_img)\n    for i in temp_files:\n        os.remove(i) \n    # remove all videos\n    temp_path_vid = \'$TEMP/*.mp4\'\n    temp_files = glob.glob(temp_path_vid)\n    for i in temp_files:\n        os.remove(i) \n\ndef create_geo(node):\n    geo = node.createNode("geo", "geometry")\n    \n    return geo\n\ndef create_python(geo):\n    python = geo.createNode("python", "python")\n\n    return python\n\n\n\ndef object_tracking(python):\n    imagesToTrack = "$TEMP/mv_$F3.png"\n    cap = cv2.VideoCapture(imagesToTrack)\n\n    # object detection from stable camera\n    object_detector = cv2.createBackgroundSubtractorMOG2(history=100, varThreshold=5)\n    \n    code = \'\'\'\n\n\nnode = hou.pwd()\ngeo = node.geometry()\n\nimport cv2\n\nvideo = "$TEMP/mv.mp4"\n\ncap = cv2.VideoCapture(video)\n\n# object detection from stable camera\nobject_detector = cv2.createBackgroundSubtractorMOG2(history=30, varThreshold=6)\n\n\ngeo.addAttrib(hou.attribType.Point, "x",0)\ngeo.addAttrib(hou.attribType.Point, "y",0)\ngeo.addAttrib(hou.attribType.Point, "w",0)\ngeo.addAttrib(hou.attribType.Point, "h",0)\ngeo.addAttrib(hou.attribType.Point, "i",0)\n\ni = 0\nwhile True:\n    i +=1\n    ret, frame = cap.read()\n\n    height, width, _ = frame.shape\n    \n    # extract region of interst\n    roi = frame[0:450,0:800]\n    \n\n\n    # obejct detection\n    mask = object_detector.apply(frame)\n    contours, _ = cv2.findContours(mask, cv2.RETR_TREE, cv2.CHAIN_APPROX_SIMPLE)\n\n\n   \n    for cnt in contours:\n        # calculate area and remove smalle element\n        area = cv2.contourArea(cnt)\n\n        \n        if area > 500:\n            cv2.drawContours(frame, [cnt], -1, (0,255,0),2)\n            x,y,w,h = cv2.boundingRect(cnt)\n            cv2.rectangle(roi, (x,y), (x+w,y+h), (0,255,0),3)\n            rectangle = frame[y:y+h, x:x+w]\n            \n            \n            # create houdini point\n            \n            pnt = geo.createPoint()\n\n            # pos = hou.Vector4(x,y,w,h)\n            pnt.setAttribValue("x",x)\n            pnt.setAttribValue("y",y)\n            pnt.setAttribValue("w",w)\n            pnt.setAttribValue("h",h)\n            \n            pnt.setAttribValue("i",i)\n            # pnt.setAttribValue("y", x)\n            # pnt.setAttribValue("w", x)\n            # pnt.setAttribValue("h", x)\n           \n            \n    \n\n    # cv2.imshow("Roi", roi)\n    # cv2.imshow("Frame", frame)\n    # cv2.imshow("Mask", mask)\n    cv2.imwrite(f"$HIP/roi/roi_{i}.png", rectangle)\n\n    key = cv2.waitKey(30)\n    if i == $FEND-1:\n        break\n\ncap.release()\ncv2.destroyAllWindows()\n    \n\n# #set points in Houdini\n# #set attribute\n# geo.addAttrib(hou.attribType.Point, "newPos", hou.Vector3(0,0,0))\n\n# #create point\n# pntb = geo.createPoint()\n# newPos = hou.Vector3(5,4,3)\n\n# pntb.setAttribValue("newPos",newPos)\n\n# P = hou.Vector3(0,1,2)\n# pntb.setAttribValue("P", P)\n\n\n\n    \'\'\'\n    \n    python.parm("python").set(code)\n    \n\n\n\ndef rest_of_nodes(node, cam):\n\n    geo = hou.node(node.path()+"/geometry")\n    python = hou.node(node.path()+"/geometry/python")\n\n    aw1 = geo.createNode("attribwrangle", "attribwrangle1")\n    snippet1 = \'\'\'\nfloat x = f@x;\nfloat y = f@y;\nfloat w = f@w;\nfloat h = f@h;\nint i = @i;\n\n//set position\nv@P = set(x+w/2,y+h/2,0);\n\n//set scale attribute\n@scale = set(h,w,1);\n\n//set color\nv@Cd = set(0,0,1);\n\n//if there are points onf v@P.x ==0, then remove\nif(@P.x ==0) removepoint(0,@ptnum);\n\n//delete if not on frame\nif(i!=@Frame || i==1) removepoint(0,@ptnum);\n\'\'\'\n    aw1.parm("snippet").set(snippet1)\n    aw1.setInput(0, python)\n\n    grid = geo.createNode("grid", "grid")\n    grid.setParms({\'sizex\':1, \'sizey\':1, \'rows\':2, \'cols\':2, \'orient\':0})\n\n    ends = geo.createNode("ends", "ends")\n    ends.parm("closeu").set(5)   \n    ends.setInput(0, grid)\n\n    copyToPoints = geo.createNode("copytopoints::2.0")\n    # copyToPoints.parm("resettargetattribs").pressButton()\n    copyToPoints.setInput(0, ends)\n    copyToPoints.setInput(1, aw1)\n\n    aw2 = geo.createNode("attribwrangle", "attribwrangle2")\n    camy = cam.evalParm("resy")\n    camx = cam.evalParm("resx")\n    snippet2=f\'\'\'\nv@P.x /={camx};\nv@P.y = ((v@P.y/{camy})*-1)+1;\n\nstring cam = "/obj/{cam}";\nv@P.z = -0.619;\nv@P = fromNDC(cam,v@P);\nv@Cd = set(0,0,1);\n\'\'\'\n    aw2.parm("snippet").set(snippet2)\n    aw2.setInput(0, copyToPoints)\n\n    null = geo.createNode("null")\n    null.setInput(0, aw2)\n    null.setDisplayFlag(True)\n    \n\ndef layout_nodes(node):\n    \n    geo = hou.node(node.path()+"/geometry")\n    ropnet = hou.node(node.path()+"/ropnet")\n    geo.layoutChildren()\n    ropnet.layoutChildren()\n    node.layoutChildren()\n\n\n\ndef main():\n    remove_temp_files()\n    destroy_children(node)\n    create_ropnet_and_opengl(node, cam, temp_image_path)\n    render_opengl(node)\n    render_video()\n    object_tracking(create_python(create_geo(node)))\n    rest_of_nodes(node, cam)\n    layout_nodes(node)\n    \n\nmain()\n' )
