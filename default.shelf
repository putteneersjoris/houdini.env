 if there are multiple nodes with the same name\n            list.append(i.name())\n            if len(list) > 1:\n                rivetSop = i.path()\n            else:\n                rivetSop = i.path()\n                \n    # create and set rivet node\n    rivet = node.createNode(\'rivet\')\n    rivet.setParms({\'rivetsop\': rivetSop, \'rivetgroup\': "0", \'rivetuseattribs\': "1"})\n\n    # create and set ropnet and opengl rops\n    ropnet = node.createNode("ropnet")\n\n    # create and set copnetwork\n    copnet = node.createNode("cop2net")\n    colorSop = copnet.createNode("color")\n    colorSop.setParms({\'overridesize\': 1 , \'size1\': resolutionY*2, \'size2\': resolutionY })\n    # composite = copnet.createNode("composite")\n    # composite.setInput(1, colorSop)\n    \n    copList = []  \n    for i in range(n_cam):\n        \n        cam = node.createNode("cam")\n        # print(cam.path())\n        cam.parm("ry").set(angle * i + offset)\n        \n        cam.setParms({\'resx\': int(resolutionX), \'resy\': int(resolutionY), "aperture": 50})\n        cam.parm("focal").set(focalLength)\n\n        cam.setDisplayFlag(False)\n        cam.setInput(0, rivet)\n\n        # make opengl rops\n        opengl = ropnet.createNode("opengl")\n        opengl.setParms({\'camera\': cam.path()})\n\n        # print(hou.intFrame())\n\n        \n        opengl.parm("picture").set(f"$TEMP/{i}_\\$F.png")\n        opengl.parm("trange").set(1)\n        opengl.parm("f1").set(framerangex)\n        opengl.parm("f2").set(framerangey)\n        opengl.parm("gamma").set(2.2)\n        # opengl.parm("shadingmode").set(0)\n        # opengl.parm("motionblur").set(1)\n        # opengl.parm("motionblurframes").set(30)\n        opengl.parm("particle").set("pixels")\n        # opengl.parm("discsize").set(1)\n        opengl.parm("execute").pressButton()\n        # a lot more parms to adjust\n\n        # create composite nodes\n        newComposite = copnet.createNode("composite")\n        copList.append(newComposite.path())\n\n        newComposite.parm("op").set("screen")\n        newComposite.parm("units").set("pixel")\n        newComposite.parm("px").set(0.5)\n        newComposite.parm("py").set(0.5)\n        newComposite.parm("tx").set(math.ceil((resolutionY*2) - int(resolutionX * i) - (resolutionX)))\n        \n        file = copnet.createNode("file")\n        file.parm("filename1").set(f"$TEMP/{i}_\\$F.png")\n        \n        \n        \n\n        if i !=0:\n            newComposite.setInput(1, hou.node(copList[i-1]))\n            newComposite.setInput(0, file)\n            \n        else:\n            newComposite.setInput(1, colorSop)\n            newComposite.setInput(0, file)\n            newComposite.parm("op").set("min")\n\n        # newComposite.setParms({\'op\': 5 ,  \'units\': 1 })\n        \n        \n\n    ropComp = copnet.createNode("rop_comp")\n    ropComp.setInput(0, hou.node(copList[n_cam-1]))\n\n\n\n    # ropComp.parm("f2").set(10)\n    ropComp.parm("f1").set(framerangex)\n    ropComp.parm("f2").set(framerangey)\n    \n    ropnet.layoutChildren()\n    node.layoutChildren()\n    copnet.layoutChildren()\n\n\n    ropComp.parm("copoutput").set(output.replace(".","_\\$F3."))\n    # ropComp.parm("copoutput").set("ip")\n    ropComp.parm("execute").pressButton()\n\n        \n\n        \n        \n       \n        \n\n\n        \n\n\n' ) opengl_out ( '$HIP/blabla/a.png' ) python_flip ( 'import hou\nimport toolutils\nimport math\n\n\n\nobj = hou.node(\'/obj\')\nnode = hou.pwd()\n\nn_cam = node.evalParm("n_cam")\nresolutionY = node.evalParm("output_resolution_y")\nresolutionX = (resolutionY*2)/n_cam\nframerangex = node.evalParm("framerangex")\nframerangey = node.evalParm("framerangey")\noutput = node.evalParm("flip_out")\nviewer = toolutils.sceneViewer()\nview = viewer.curViewport()\n\ncur_desktop = hou.ui.curDesktop()\nscene_viewer = hou.paneTabType.SceneViewer\n\nscene = cur_desktop.paneTabOfType(scene_viewer)\n# scene.flipbookSettings().stash(